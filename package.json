    "@qavajs/memory": "^1.6.2",
    "@qavajs/po-playwright": "^0.11.0",
    "@qavajs/soft-assertion": "^0.0.1",
    "@qavajs/steps-api": "^0.16.0",
    "@qavajs/steps-files": "^0.7.0",
    "@qavajs/steps-memory": "^0.13.0",
    "@qavajs/steps-playwright": "^0.30.0",
