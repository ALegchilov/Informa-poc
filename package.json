{
  "name": "informa-poc",
  "version": "0.0.1",
  "description": "qavajs-based test automation framework",
  "scripts": {
    "test": "npx ts-node --project tsconfig.json node_modules/@qavajs/cli/bin/qavajs.js run --config config.ts",
